{
  "rewrites": [
    {
      "source": "/((?!api|_next|_static|_vercel|favicon.ico|robots.txt|sitemap.xml|sw.js|service-worker.js|workbox-*).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options", 
          "value": "nosniff"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}