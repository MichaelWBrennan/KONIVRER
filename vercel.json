{
  "version": 2,
  "routes": [
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^/cards$", "dest": "/cards.html" },
    { "src": "^/deckbuilder$", "dest": "/deckbuilder.html" },
    { "src": "^/how-to-play$", "dest": "/how-to-play.html" },
    { "src": "^/about$", "dest": "/about.html" },
    { "src": "^/contact$", "dest": "/contact.html" },
    { "src": ".*", "dest": "/404.html", "status": 404 }
  ]
}
