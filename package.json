{
  "name": "konivrer-deck-database",
  "version": "1.0.0",
  "description": "Professional deck building and card database for KONIVRER trading card game",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 12000",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 12001",
    "start": "vite --host 0.0.0.0 --port 12000",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "automation:run": "node scripts/enterprise-automation.js --run",
    "automation:security": "node scripts/enterprise-automation.js --security-only",
    "automation:performance": "node scripts/enterprise-automation.js --performance-only",
    "security:check": "node scripts/security-check.js",
    "security:audit": "npm audit --audit-level moderate",
    "security:scan": "npm audit && node scripts/security-check.js",
    "security:full": "npm run security:audit && npm run security:check",
    "quality:check": "bash scripts/quality-check.sh",
    "performance:optimize": "node scripts/optimize-performance.js",
    "build:analyze": "npm run build && node scripts/analyze-bundle.js",
    "bundle:analyze": "node scripts/analyze-bundle.js",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:audit": "npm audit fix",
    "automation:full": "npm run security:full && npm run quality:check && npm run build:analyze",
    "heal": "node scripts/auto-heal.js",
    "heal:quick": "node scripts/auto-heal.js --quick",
    "heal:full": "node scripts/auto-heal.js --full",
    "fix:all": "npm run heal && npm run lint:fix && npm run format",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "health:check": "npm run lint && npm run format:check && npm run deps:audit && npm run build",
    "self:heal": "npm run heal && npm run fix:all && npm run health:check"
  },
  "keywords": [
    "konivrer",
    "deck-builder",
    "card-game",
    "database",
    "react",
    "vite"
  ],
  "author": "KONIVRER Team",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-react": "^4.6.0",
    "audit-ci": "^7.1.0",
    "autoprefixer": "^10.4.16",
    "depcheck": "^1.4.7",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-security": "^3.0.1",
    "jscpd": "^4.0.5",
    "license-checker": "^25.0.1",
    "npm-check-updates": "^18.0.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^4.1.10"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@react-spring/web": "^10.0.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/rollup-linux-x64-gnu": "^4.44.0",
    "@tensorflow/tfjs": "^4.15.0",
    "@types/three": "^0.177.0",
    "axios": "^1.10.0",
    "cannon-es": "^0.20.0",
    "cssnano": "^7.0.7",
    "framer-motion": "^12.19.2",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.525.0",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.59.0",
    "react-qr-code": "^2.0.16",
    "react-router-dom": "^7.6.3",
    "react-spring": "^10.0.1",
    "recharts": "^3.0.0",
    "semver": "^7.7.2",
    "socket.io-client": "^4.7.4",
    "three": "^0.177.0",
    "tone": "^15.1.22",
    "tslib": "^2.6.2",
    "vite": "^7.0.0",
    "vitest": "^3.2.4",
    "web-vitals": "^4.2.4",
    "workbox-window": "^7.0.0",
    "zod": "^3.25.67"
  }
}
